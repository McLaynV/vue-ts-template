{
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 2
    },
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 2
    },
    "eslint.validate": [
        "vue",
        "javascript",
        "typescript"
    ],
    "prettier.trailingComma": "all",
    "prettier.arrowParens": "always",
    "volar.tsPlugin": true,
}
